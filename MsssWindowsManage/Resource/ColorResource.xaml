<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="TitleBackgroundBrush" Color="#3399CC" />
    <SolidColorBrush x:Key="ContentBackgroundBrush" Color="#99CCFF" />
    <SolidColorBrush x:Key="FunBackgroundBrush" Color="#CCFFFF" />
    <SolidColorBrush x:Key="BorderBrush" Color="#CCCCCC" />
    <SolidColorBrush x:Key="TextBrush" Color="#003366" />
    <LinearGradientBrush x:Key="TitleTextBrush" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#99CCFF" Offset="0"></GradientStop>
        <GradientStop Color="#FFCCCC" Offset="0.3"></GradientStop>
        <GradientStop Color="#FFCC99" Offset="1"></GradientStop>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="TitleBrush" Color="#FFFFFF" />

</ResourceDictionary>